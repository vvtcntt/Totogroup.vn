@model TOTOGROUP.Models.tblConfig
<div id="Banner">
    <div id="Left_Banner">
        <div id="Logo">
            <a href="/" title="Trang chủ" rel="nofollow">
                <img src="@Model.Logo" alt="logo TOTO" title="Logo TOTO" />
            </a>
        </div>
        <div id="Chinhhang"></div>
    </div>
    <div id="Right_Banner">
        <div id="Top_RightBanner">
            <a href="/2/Tin-tuc" title="Tin tức" rel="nofollow">Tin tức</a>
            <a href="#" title="Giới thiệu" rel="nofollow">Giới thiệu</a>
            <a href="/Lien-he" title="Liên hệ" rel="nofollow">Liên hệ</a>
            <a href="/Ban-do" title="Bản đồ" rel="nofollow">Bản đồ</a>
            <a href="#" class="en" title="Tiếng anh"><span></span></a>
            <a href="#" class="vn" title="Tiếng anh"><span></span></a>
        </div>
        <div id="Buttom_RightBanner">
             
               @{Html.RenderAction("PartialSearch", "Header");}
            
            <div id="Box_Hotline">

               
                    <span>@Model.PbxSell (*)</span>

               


                
            </div>
            <div id="Box_Order">

                @if (Session["soluong"] != "" && Session["soluong"] != null)
                {
                    
    <a href="/Order/OrderIndex" title="Giỏ hàng" class="CountOf"> <span></span> Giỏ hàng : @Session["soluong"].ToString()  </a>
                }
                else
                {
                    <a href="/Order/OrderIndex" title="Giỏ hàng" class="CountOf"> <span></span> Giỏ hàng : 0   </a>
                }
                
            </div>
        </div>
    </div>
</div>
