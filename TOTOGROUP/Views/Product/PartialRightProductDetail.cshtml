@model TOTOGROUP.Models.tblConfig
<div id="RightProduct">
    <div id="Support">
        <div id="img_Support"></div>

        <div id="Contact_Support">
            <div class="timeWorks">
                <span class="tm1">Thời gian làm việc : <strong>8H00 - 19H30</strong> </span>
                <span class="tm2">Làm việc tất cả các ngày trong tuần</span>
                <span class="tm3">
                    Khách hàng mua hàng sau <strong>19H30</strong> cần đặt lịch trước qua số hotline : <strong>1900 63 66 95</strong>
                </span>
            </div>
            <div class="hotlineSwitchBoard" style="margin:10px ">
                <span class="nameHotline">Tổng đài bán hàng</span>
                <span class="hotlines">@Model.HotlineOUT</span>
            </div>
                <span class="Hotline"><span></span> @Model.HotlineIN</span>
           
            
            <span class="Mobile"><span></span> @Model.MobileIN</span>
            <span class="Email"><span></span> @Model.Email</span>
            <span class="Address"><span></span> @Model.Address</span>
        </div>
        <div id="ListSupport">

            <span class="httt">Hỗ trợ trực tuyến</span>
       @Html.Raw(ViewBag.chuoisupport)
            <div class="line-Support"></div>
        </div>
    </div>
    <div id="MenuProduct">
        <div class="nVar_Left">
            <span>Danh mục liên quan</span>
        </div>
        <div id="Content_MenuProduct">
            @Html.Raw(ViewBag.chuoimenu)

        </div>
    </div>
    <div id="ProductLienquan">
        <div class="nVar_Left">
            <span>Sản phẩm được xem nhiều nhất</span>
        </div>
        <div id="ContentProductlienquan">
            @Html.Raw(ViewBag.chuoiproduct)
        </div>
    </div>
</div>
