            <span class="Hotline"><span></span> @Model.Hotline2</span>
            <span class="Address"><span></span> @Model.Address1</span>
            <span class="Mobile"><span></span> @Model.Mobile1</span>
            <span class="Address"><span></span> @Model.Address2</span>
            <span class="Mobile"><span></span> @Model.Mobile2</span>

            <span class="Email"><span></span> @Model.Email</span>
        </div>
        <div id="ListSupport">
            <span class="httt">Hỗ trợ trực tuyến</span>
            @Html.Raw(ViewBag.chuoisupport)

        </div>
