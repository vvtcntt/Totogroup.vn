@model TOTOGROUP.Models.tblConfig

<div id="RightProduct">
    <div id="Support">
        <div id="img_Support"></div>

        <div id="Contact_Support">
            <div class="timeWorks">
                <span class="tm1">Thời gian làm việc : <strong>8H00 - 19H30</strong> </span>
                <span class="tm2">Làm việc tất cả các ngày trong tuần</span>
                <span class="tm3">
                    Khách hàng mua hàng ngoài <strong>19H30</strong> cần đặt lịch trước qua số hotline : <strong>1900 63 66 95</strong>
                </span>
            </div>
            <div class="hotlineSwitchBoard" style="margin:10px ">
                <span class="nameHotline">Tổng đài bán hàng</span>
                <span class="hotlines">@Model.PbxSell</span>
            </div>
            <span class="Address"><span></span> @Model.Address1</span>
            <br />
